    <string-array name="stylus_button_entries" translatable="false">
        <item>@string/stylus_button_disable</item>
        <item>@string/stylus_button_default</item>
        <!-- <item>@string/stylus_button_updown</item> -->
        <item>@string/stylus_button_volume</item>
        <item>@string/stylus_button_music</item>
    </string-array>

    <string-array name="stylus_button_values" translatable="false">
        <item>disable</item>
        <item>default</item>
